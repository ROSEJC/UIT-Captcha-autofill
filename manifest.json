{
  "manifest_version": 3,
  "name": "UIT CAPTCHA Solver",
  "version": "1.0",
  "description": "Automatically solves text-based CAPTCHA questions",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://daa.uit.edu.vn/sinhvien/kqhoctap"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
